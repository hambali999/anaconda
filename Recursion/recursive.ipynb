    "def recursive(n):\n",
    "    if n < 1:\n",
    "        print(\"The number is below 1 now\")\n",
    "        print(n)\n",
    "    else:\n",
    "        # print(n)\n",
    "        recursive(n-1)\n",
    "        print(n)\n",
    "\n",
    "\n",
    "recursive(5)"
